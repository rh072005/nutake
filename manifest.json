{
    "manifest_version": 2,
    "name": "NuTake",
    "description": "Add 'download zip' links to package pages on the Nuget.org website.",
    "version": "0.4.1",
    "content_scripts": [
        {
              "matches": ["*://*.nuget.org/packages/*"],
              "js": ["contrib/jquery-2.0.2.min.js", "lib/nutake.js"],
              "run_at": "document_end"
        }
    ]
}
